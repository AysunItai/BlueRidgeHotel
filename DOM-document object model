        <mxCell id="05BW4UVMqHwpVsaxVWE3-1" value="&lt;h1&gt;DOCUMENT OBJECT MODEL&lt;/h1&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;align=center;" vertex="1" parent="1">
